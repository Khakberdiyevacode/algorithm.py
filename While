#while-20    
n = input()
while True:
    if '2' in n:
        print("yes")
        break
    else:
        print('no')
        break


#********************************************************************

#while-21
n = int(input())
v =0
while v<n:
    v +=1
    if v%2!=0:
        print(v)
       


#while-1
a,b =map(int,input().split())
while a>b :
    a-=b
print(a)

#********************************************************************

#while-2
a,b =map(int,input().split())
s=0
while a>=b:
    a-=b
    s+=1
print(s)

#********************************************************************

#while-3
a,b =map(int,input().split())
s=0
while a>=b:
    a-=b
    s+=1
print(s,a,'qoldiq')
break

#********************************************************************

#while-4
n = int(input())
s = 1
a = True
while a:
    s *= 3
    if n==s:
        print('3 ning darajasi')
        break
    if s > n :
        a = False
        print("3 ning darajasi emas")

#********************************************************************

#while-5
a=0
k = 1
n = int(input())
while n>0:
    k*=2
    n-=k
    a+=1
print(f"2 ning {a} darajasi {2**a}") 
    
#********************************************************************

#while_7
k=0
n = int(input())
while True:
    k+=1
    a=k**2
    if n<a:
        print(f"{a**(1/2)} ning kvdarti {a} katta")
        break

#********************************************************************

#while_8
n = int(input())
x =0
while n > x:
    x+=1
    if n < x**2:
        print(x-1)
        break

#********************************************************************

#while-9
s = 1
n = int(input())
while True:
    s*=3
    if n < s:
        print(s)
        break
    
#********************************************************************

#while-10
n = int(input())
s = 1
while True:
    s*=3
    if s >=n :
        print(s//3)
        break
       
#********************************************************************

#while-11
n = int(input())
a = 0
b = 0
while True:
    a+=1
    b+=a
    if b >= n :
        print(b,a)
        break
    
#********************************************************************

#while-12
n = int(input())
a = 0
b = 0
while n>b:
    a+=1  
    b+=a
print(b-a,a-1)
 
#********************************************************************

#while-13
n = int(input())
a = 0
b = 1
while True:
    a+=1
    b+=1/a
    if b >= n :
        break
print(b,a)
    
#********************************************************************

#while-14
